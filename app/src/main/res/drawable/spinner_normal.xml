<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
       
            
      <solid
          android:color="#A7AAAE" />
      <corners
          android:radius="6dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>
  <item>
      
    <shape>
       
      <gradient
          android:startColor="#A7AAAE"
          android:endColor="#A7AAAE"
          android:angle="270" />
       
      <corners
          android:radius="3dp" />
       
      <padding
          android:left="5dp"
          android:top="5dp"
          android:right="5dp"
          android:bottom="5dp" />
    </shape>
  </item>
</selector>
